{
  "name": "hikibiki-app",
  "version": "0.0.1",
  "description": "Look up words and stuff",
  "main": "src/main.tsx",
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@relative-ci/agent": "3.1.3",
    "@storybook/preact": "6.4.22",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "10.2.4",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "mini-css-extract-plugin": "2.6.0",
    "postcss-import": "14.1.0",
    "postcss-loader": "6.2.1",
    "prettier": "2.6.2",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.0",
    "typescript": "4.6.4",
    "webpack": "5.72.1",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.0",
    "workbox-webpack-plugin": "6.5.3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "build:stats": "cross-env NODE_ENV=production webpack --json > webpack-stats.json",
    "build:storybook": "build-storybook -c .storybook -s dist",
    "postinstall": "husky install",
    "start": "webpack serve",
    "storybook": "start-storybook -p 9001 -c .storybook -s dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birchill/hikibiki-app.git"
  },
  "keywords": [
    "kanji",
    "漢字"
  ],
  "author": "Brian Birtles",
  "bugs": {
    "url": "https://github.com/birchill/hikibiki-app/issues"
  },
  "homepage": "https://hikibiki.app/",
  "dependencies": {
    "@birchill/jpdict-idb": "^0.0.3",
    "@birchill/normal-jp": "^1.4.0",
    "idb-keyval": "^6.1.0",
    "postcss": "^8.4.12",
    "preact": "^10.6.6",
    "rollbar": "^2.24.1",
    "tailwindcss": "^3.0.23"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "goodBundle": {
    "assets": {
      "hikibiki.js": "./dist/hikibiki.*.js",
      "worker.js": "./dist/worker.*.js",
      "hikibiki.css": "./dist/hikibiki.*.css"
    },
    "output": {
      "format": [
        "csv",
        "json"
      ],
      "project": "hikibiki",
      "bucket": "bundlesize-stack-bundlestatsbucket-15h4d77z8q1hr",
      "destDir": "hikibiki",
      "region": "us-west-2"
    },
    "stats": "webpack-stats.json"
  }
}
