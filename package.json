{
  "name": "hikibiki-app",
  "version": "0.0.1",
  "description": "Look up words and stuff",
  "main": "src/main.tsx",
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@relative-ci/agent": "4.1.3",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/preact": "6.5.16",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "mini-css-extract-plugin": "2.7.3",
    "postcss": "8.4.21",
    "postcss-import": "15.1.0",
    "postcss-loader": "7.0.2",
    "prettier": "2.8.4",
    "style-loader": "3.3.1",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "webpack": "5.76.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "workbox-webpack-plugin": "6.5.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "build:stats": "cross-env NODE_ENV=production webpack --json > webpack-stats.json",
    "build:storybook": "build-storybook -c .storybook",
    "postinstall": "husky install",
    "start": "webpack serve",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birchill/hikibiki-app.git"
  },
  "keywords": [
    "kanji",
    "漢字"
  ],
  "author": "Brian Birtles",
  "bugs": {
    "url": "https://github.com/birchill/hikibiki-app/issues"
  },
  "homepage": "https://hikibiki.app/",
  "dependencies": {
    "@birchill/jpdict-idb": "^1.2.2",
    "@birchill/normal-jp": "^1.4.0",
    "idb-keyval": "^6.2.0",
    "preact": "^10.10.6",
    "rollbar": "^2.25.1",
    "tailwindcss": "^3.1.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "goodBundle": {
    "assets": {
      "hikibiki.js": "./dist/hikibiki.*.js",
      "worker.js": "./dist/worker.*.js",
      "hikibiki.css": "./dist/hikibiki.*.css"
    },
    "output": {
      "format": [
        "csv",
        "json"
      ],
      "project": "hikibiki",
      "bucket": "bundlesize-stack-bundlestatsbucket-15h4d77z8q1hr",
      "destDir": "hikibiki",
      "region": "us-west-2"
    },
    "stats": "webpack-stats.json"
  }
}
